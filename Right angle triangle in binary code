#include<stdio.h>
 
int main() 
{
   int i, j;
   int count = 0;
 
   for (i = 2; i <= 10; i=i+2) 
   {
      printf("\n");
      for (j = 1; j <= i; j++) 
      {
         printf("%d", count % 2);
         count++;
      }
 
      if (i % 2 == 0)
         count = 0;
      else
         count = 0;
   }
   return(0);
}
